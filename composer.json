{
  "name": "luckycode/package",
  "description": "Redeemly LuckyCode Integration Helper: framework-agnostic PHP client with optional Laravel bridge",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "guzzlehttp/guzzle": "^7.9",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/contracts": "^11.0|^12.0",
    "illuminate/routing": "^11.0|^12.0",
    "illuminate/config": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "LuckyCode\\IntegrationHelper\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LuckyCode\\IntegrationHelper\\IntegrationHelperServiceProvider"
      ]
    }
  },
  "repositories": {
    "luckycode": {
      "type": "path",
      "url": "..\\"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
